{
  "name": "ddrm",
  "version": "0.1.0",
  "author": "EventGuru team",
  "description": "SerialHacking task",
  "keywords": [
    "eventguru",
    "drm",
    "ethereum",
    "erc20",
    "erc165",
    "erc721"
  ],
  "homepage": "https://github.com/aviacore/ddrm",
  "repository": {
    "type": "git",
    "url": "https://github.com/aviacore/ddrm.git"
  },
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:sol && npm run build:js",
    "build:js": "node scripts/build.js",
    "build:sol": "./node_modules/.bin/truffle compile --all",
    "chain": "./node_modules/.bin/ganache-cli",
    "console": "./node_modules/.bin/truffle console --network development",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "lint:js": "./node_modules/.bin/prettier --config .prettierrc.json --ignore-path .prettierignore \"**/*.js\" \"**/*.jsx\"",
    "lint:js:fix": "./node_modules/.bin/prettier --config .prettierrc.json --ignore-path .prettierignore --write \"**/*.js\" \"**/*.jsx\"",
    "lint:sol": "./node_modules/.bin/solium -d .",
    "lint:sol:fix": "./node_modules/.bin/solium -d . --fix",
    "migrate": "./node_modules/.bin/truffle migrate --network development --reset",
    "start": "node scripts/start.js",
    "test": "npm run test:js && npm run test:sol",
    "test:js": "node scripts/test.js --env=jsdom",
    "test:sol": "./node_modules/.bin/truffle test --network development"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.2.0",
    "chalk": "2.4.1",
    "css-loader": "0.28.7",
    "dotenv": "^6.1.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "eth-block-tracker-es5": "^2.3.2",
    "ethjs-abi": "^0.2.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "ganache-cli": "^6.1.6",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "object-assign": "4.1.1",
    "openzeppelin-solidity": "^2.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.15.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.1",
    "resolve": "1.6.0",
    "solium": "^1.1.8",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "truffle": "^4.1.14",
    "truffle-hdwallet-provider": "0.0.6",
    "url-loader": "0.6.2",
    "web3-utils": "^1.0.0-beta.36",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "drizzle": "^1.2.5",
    "drizzle-react": "^1.1.1",
    "drizzle-react-components": "^1.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "web3": "^1.0.0-beta.34"
  }
}
